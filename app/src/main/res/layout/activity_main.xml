<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/name"
    android:background="#FFFFFF">

    <TextView android:text="5olution!" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <RatingBar
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:id="@+id/ratingBar"
        android:layout_alignParentLeft="true"
        android:layout_toStartOf="@+id/button9"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button9" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_below="@+id/button2"
        android:layout_centerHorizontal="true"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="plants"
        android:id="@+id/button9"
        android:layout_alignTop="@+id/button8"
        android:layout_alignLeft="@+id/button6"
        android:layout_gravity="right|bottom"
        android:onClick="PlantsButton"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="appliances"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center|right|top"
        android:onClick="ApplianceButton"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="water"
        android:id="@+id/button5"
        android:layout_gravity="center"
        android:layout_above="@+id/chronometer4"
        android:layout_alignLeft="@+id/button2"
        android:onClick="WaterButton"
        android:layout_alignRight="@+id/button2"
        android:layout_alignEnd="@+id/button2"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="transit"
        android:id="@+id/button6"
        android:layout_gravity="right|center_vertical"
        android:layout_alignTop="@+id/button5"
        android:layout_alignLeft="@+id/button3"
        android:singleLine="true"
        android:onClick="Activity2Go" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="paper"
        android:id="@+id/button7"
        android:layout_gravity="left|bottom"
        android:onClick="PaperButton"
        android:layout_below="@+id/chronometer4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="50dp"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="car"
        android:id="@+id/button4"
        android:layout_gravity="left|center_vertical"
        android:layout_below="@+id/chronometer2"
        android:layout_alignParentLeft="true"
        android:onClick="CarButton"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="34dp"
        android:layout_height="39dp"
        android:id="@+id/chronometer2"
        android:layout_gravity="center_horizontal|bottom|right"
        android:layout_below="@+id/button"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button"
        android:layout_alignLeft="@+id/chronometer4"
        android:layout_alignStart="@+id/chronometer4"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cans"
        android:id="@+id/button8"
        android:layout_alignTop="@+id/button7"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5"
        android:layout_gravity="center_horizontal|bottom"
        android:onClick="CansButton"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="electronics"
        android:id="@+id/button2"
        android:layout_gravity="center|top"
        android:layout_above="@+id/chronometer2"
        android:layout_centerHorizontal="true"
        android:onClick="ElectronicsButton"
        android:singleLine="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lights"
        android:id="@+id/button"
        android:layout_gravity="left|top"
        android:onClick="LightButton"
        android:layout_below="@+id/scoretext"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer3"
        android:layout_below="@+id/button3"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer4"
        android:layout_toLeftOf="@+id/button2"
        android:layout_below="@+id/button4"
        android:layout_alignRight="@+id/button7"
        android:layout_toEndOf="@+id/textView"
        android:layout_alignLeft="@+id/scoretext"
        android:layout_alignStart="@+id/scoretext"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer5"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/button5"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer6"
        android:layout_below="@+id/button5"
        android:layout_alignRight="@+id/button6"
        android:layout_alignBottom="@+id/chronometer5"
        android:layout_alignLeft="@+id/chronometer3"
        android:singleLine="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="SCORE"
        android:id="@+id/scoretext"
        android:singleLine="true"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/button13"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HIGH SCORES"
        android:id="@+id/button13"
        android:layout_alignTop="@+id/textView"
        android:layout_alignRight="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:onClick="HighScoreMaker"
        android:singleLine="true" />

    <Chronometer
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chronometer7"
        android:layout_above="@+id/ratingBar"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="46dp"
        android:singleLine="true" />

</RelativeLayout>